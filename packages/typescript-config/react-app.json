{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-native",
    "lib": ["ESNext"],
    "module": "ESNext",
    "target": "esnext",
    "types": ["react", "react-native", "jest"],
